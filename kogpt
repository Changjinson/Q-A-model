"source":["K_TKN = '<usr>'\n","E_TKN = '<sys>'\n","BOS = '</s>'\n","EOS = '</s>'\n","MASK = '<unused0>'\n","SENT = '<unused1>'\n","PAD = '<pad>'"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"Pc1VqCgwe1Lu"},"outputs":[],"source":["from torch.utils.data import DataLoader, Dataset\n","import re\n","\n","class translate_dataset(Dataset):\n","  def __init__(self, data_translate, TOKENIZER, max_length=150):\n","    self.data = data_translate\n","    self.max_length = max_length\n","    self.k_token = K_TKN\n","    self.e_token = E_TKN\n","    self.eos = EOS\n","    self.bos = BOS\n","    self.mask = MASK\n","    self.tokenizer = TOKENIZER\n","\n","    self.token_ids = []\n","    self.label_ids = []\n","    self.segments = []\n","    for i in range(len(self.data)):\n","      k_e = self.data.iloc[i]\n","\n","      k = k_e['ko']\n","      k = re.sub(r\"([?.!,])\\\"\\'\\t\", r\" \", k) \n","\n","      e = k_e['en']\n","      e = re.sub(r\"([?.!,])\\\"\\'\\t\", r\" \", e)\n","\n","      k_toked = self.tokenizer.tokenize(self.k_token + k) \n","      k_len = len(k_toked)\n","\n","      e_toked = self.tokenizer.tokenize(self.e_token + e + self.eos)\n","      e_len = len(e_toked)\n","\n","      labels = [self.mask] * k_len + e_toked[1:]\n","\n","      segment = [0] * k_len + [1] * e_len + [0] * (self.max_length - k_len - e_len)\n","\n","      labels_id = self.tokenizer.convert_tokens_to_ids(labels)\n","      while len(labels_id) < self.max_length:\n","            labels_id += [self.tokenizer.pad_token_id]\n","\n","      token_id = self.tokenizer.convert_tokens_to_ids(k_toked + e_toked)\n","      while len(token_id) < self.max_length:\n","          token_id += [self.tokenizer.pad_token_id]\n","\n","      self.token_ids.append(token_id)\n","      self.label_ids.append(labels_id)\n","      self.segments.append(segment)\n","\n","  def __len__(self):\n","    return len(self.data)\n","\n","  def __getitem__(self, idx):\n","    return (self.token_ids[idx], self.label_ids[idx], self.segments[idx])"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"wg64_OQ_6TMc"},"outputs":[],"source":["def collate_fn(batch):\n","  data = [item[0] for item in batch]\n","  label = [item[1] for item in batch]\n","  mask = [item[2] for item in batch]\n","  return torch.LongTensor(data), torch.LongTensor(mask), torch.LongTensor(label)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"OOTjTzei1nWx"},"outputs":[],"source":["translate_data = translate_dataset(data_BT, TOKENIZER, max_length=150)\n","train_dataloader = DataLoader(translate_data, collate_fn=collate_fn, batch_size=15)"]},{"cell_type":"markdown","source":["# training"],"metadata":{"id":"ww2bi3NMlEYo"}},{"cell_type":"code","execution_count":null,"metadata":{"id":"gnrue4e66ogQ"},"outputs":[],"source":["import numpy as np\n","import pandas as pd\n","import torch\n","from torch.utils.data import DataLoader, Dataset\n","from torch.optim.lr_scheduler import ExponentialLR\n","from transformers import PreTrainedTokenizerFast, GPT2LMHeadModel, AutoModelForCausalLM\n","import re\n","\n","device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","model = AutoModelForCausalLM.from_pretrained(\"skt/kogpt2-base-v2\")\n"]},{"cell_type":"code","source":["learning_rate = 3e-4\n","criterion = torch.nn.CrossEntropyLoss(reduction=\"none\")\n","optimizer = torch.optim.Adam(model.parameters(), lr=learning_rate, eps=3e-10)"],"metadata":{"id":"y3Oy7o_2NdTP"},"execution_count":null,"outputs":[]},{"cell_type":"code","execution_count":null,"metadata":{"id":"h1jW3SsOP3de"},"outputs":[],"source":["#checkpoint = torch.load('/content/drive/MyDrive/Colab Notebooks/데이터사이언스 특론/term project/translate_model/translate_model.2.pt')   # dict 불러오기\n","#model.load_state_dict(checkpoint['model_state_dict'])\n","#model.to(device)\n","#optimizer.load_state_dict(checkpoint['optimizer_state_dict'])"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"pLpWuFriJGc6"},"outputs":[],"source":["from google.colab import files\n","\n","epochs = 1\n","Sneg = -1e18\n","losses = []\n","\n","print (\"start\")\n","for epoch in range(epochs):\n","  for batch_idx, samples in enumerate(train_dataloader):\n","    optimizer.zero_grad()\n","    token_ids, mask, label = samples\n","\n","    token_ids = token_ids.to(device)\n","    mask = mask.to(device)\n","    label = label.to(device)\n","    out = model(token_ids)\n","    \n","    out = out.logits     \n","    mask_3d = mask.unsqueeze(dim=2).repeat_interleave(repeats=out.shape[2], dim=2).to(device)\n","    mask_out = torch.where(mask_3d == 1, out, Sneg * torch.ones_like(out)).to(device)\n","    loss = criterion(mask_out.transpose(2, 1), label)\n","    avg_loss = loss.sum() / mask.sum()\n","    avg_loss.backward()\n","    losses.append(avg_loss.item())\n","    torch.nn.utils.clip_grad_norm_(model.parameters(), 0.3)\n","    optimizer.step()\n","    if batch_idx % 500 == 0:\n","      print('batch - ',batch_idx, 'Loss - ', sum(losses) / len(losses))\n","  total_loss = sum(losses) / len(losses)\n","  print('============================================')\n","  print('Epoch = ', epoch)\n","  print('Loss per Epoch = ', total_loss)\n","  torch.save({'epoch': epoch, 'optimizer_state_dict': optimizer.state_dict(), 'model_state_dict': model.state_dict(), 'loss': total_loss}, '/content/drive/MyDrive/Colab Notebooks/데이터사이언스 특론/term project/translate_BT_model.{}.pt'.format(epoch+2))\n","  print('')\n","print (\"end\")\n","\n","\n","# small model epoch 10번 기준\n","# 232.2218 -> 227.4465"]},{"cell_type":"markdown","source":["# inference"],"metadata":{"id":"qKJljOf9lM35"}},{"cell_type":"code","source":["from transformers import PreTrainedTokenizerFast, GPT2LMHeadModel, AutoModelForCausalLM\n","import torch\n","device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","model = AutoModelForCausalLM.from_pretrained(\"skt/kogpt2-base-v2\").to(device)\n","checkpoint = torch.load('/content/driv
eat_ngram_size=3,\n","        num_return_sequences=1,)\n","      translation_result = tokenizer.decode(translation_result[0][1:-1])\n","      pred_sentence.append(translation_result)\n","      print('original sentence : ', data_ko['en'][i])\n","      print('translated sentence quoqa :', translation_result)\n","    \n","      a = \"\"\n","      while 1:\n","          input_ids = torch.LongTensor(TOKENIZER.encode(K_TKN + q + E_TKN + a)).unsqueeze(dim=0)\n","          pred = model(input_ids)\n","          pred = pred.logits\n","          gen = TOKENIZER.convert_ids_to_tokens(torch.argmax(pred, dim=-1).squeeze().cpu().numpy().tolist())[-1]\n","          if gen == EOS:\n","              break\n","          a += gen.replace(\"▁\", \" \")\n","      print('translated sentence kogpt :', a)\n","      print()\n","\n","\n","    total_socre = 0\n","    for i in range(len(pred_sentence)):\n","      total_socre += bleu.sentence_bleu([pred_sentence[i].split()],data_ko['en'][i].split())\n","\n","    bleu_scores.append(total_socre / len(pred_sentence))\n","    print(\"model{} : bleu_score - {}\".format(num,(total_socre / len(pred_sentence))))"],"metadata":{"id":"k07-StZZ7OgC"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["total_socre = 0\n","for i in range(len(pred_sentence)):\n","    print(bleu.sentence_bleu([pred_sentence[i].split()],data_ko['en'][i].split()))\n","    total_socre += bleu.sentence_bleu([pred_sentence[i].split()],data_ko['en'][i].split())\n","\n","    #bleu_scores.append(total_socre / len(pred_sentence))\n","print(\"model{} : bleu_score - {}\".format(num,(total_socre / len(pred_sentence))))"],"metadata":{"id":"YvFopyZnGRkC"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["import nltk.translate.bleu_score as bleu\n","\n","total_socre = 0\n","for i in range(len(pred_sentence)):\n","  print('original sentence : ', data_ko['en'][i])\n","  print('translated sentence :', pred_sentence[i])\n","  total_socre += bleu.sentence_bleu([pr
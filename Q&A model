,"        outputs_QA = model(**inputs)\n","\n","        answer_start_index = outputs_QA.start_logits.argmax(dim=-1)\n","        answer_start_logits = torch.nn.Softmax(dim=1)(outputs_QA.start_logits)[0][answer_start_index]\n","\n","        answer_end_index = outputs_QA.end_logits.argmax(dim=-1)\n","        answer_end_logits = torch.nn.Softmax(dim=1)(outputs_QA.end_logits)[0][answer_end_index]\n","\n","        if answer_start_index.item() != 0 and answer_end_index != 0:\n","          end_logits.append(answer_end_logits)\n","          start_logits.append(answer_start_logits)\n","\n","          answer = tokenizer.decode(input_id[0][answer_start_index:answer_end_index+1]).strip()\n","          answers.append(answer)\n","      \n","      if len(answers) != 0:\n","        start_logits = torch.tensor(start_logits).unsqueeze(0)\n","        end_logits = torch.tensor(end_logits).unsqueeze(0)\n","        answer_index = (start_logits * end_logits).argmax(dim=1).item()\n","        pred_ans = answers[answer_index]\n","      else:\n","        pred_ans = '정답 없음'\n","      id_num = val_data['id'][i]\n","      answer_dict[id_num] = pred_ans\n","    print('===============================================')\n","    print('model {}'.format(num))\n","    performance = evaluate(val_data, answer_dict)\n","    print('EM : ', performance['exact_match'])\n","    print('F1 : ', performance['f1'])\n","    print()\n","    print('===============================================')"]},{"cell_type":"markdown","source":["#### multilingual bert 실제 점수 평가"],"metadata":{"id":"1igzzdsRRwq0"}},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":9290610,"status":"ok","timestamp":1670830511573,"user":{"displayName":"창진손","userId":"16752718039791828465"},"user_tz":-540},"id":"pS3K7l2gx8dA","outputId":"ee11acd2-5faa-4e0a-be3b-045cd5faf56b"},"outputs":[{"name":"stderr","output_type":"stream","text":["\r  0%|          | 0/10 [00:00<?, ?it/
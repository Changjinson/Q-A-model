er_start\n","df['is_impossible'] = is_impossible\n","val_data = df.sample(frac=1,random_state=1)[:10000].reset_index(drop=True)\n","val_data = val_data[:500]"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"v6FPxWUylLdI"},"outputs":[],"source":["import torch\n","\n","# input값 설정\n","def input_trans_QA(question, context, tokenizer, doc_stride=128):\n","  input_ids = []\n","  input_masks = []\n","  segment_ids = []\n","  question_len = len(tokenizer.encode(question)) -2 \n","  context_len = len(tokenizer.encode(context)) -2\n","  if context_len >= 509 - question_len:\n","    cont_len = 509 - question_len\n","    num = int(context_len / cont_len)\n","    for i in range(num+1):\n","      cont_split = tokenizer.encode(context)[1:-1]\n","      if i == 0 :\n","         stride = 0\n","      else:\n","         stride = doc_stride\n","      cont_split = cont_split[i*cont_len - stride : (i + 1)*cont_len - stride] # -> - stride 추가 \n","      sequence = tokenizer.encode(question) + cont_split + tokenizer.encode(['[SEP]'])[1:2]\n","      input_mask = [1] * len(sequence)\n","      segment_id = [0] * (question_len+2)+ [1] * (len(cont_split)+1)\n","      input_ids.append(torch.tensor([sequence]))\n","      input_masks.append(torch.tensor([input_mask]))\n","      segment_ids.append(torch.tensor([segment_id]))\n","  else:\n","    sequence = question + '[SEP]' + context\n","    sequence = tokenizer.encode(sequence)\n","    input_mask = [1] * len(sequence)\n","    segment_id = [0] * (question_len+2)+ [1] * (len(sequence) - (question_len+2))\n","    input_ids.append(torch.tensor([sequence]))\n","    input_masks.append(torch.tensor([input_mask]))\n","    segment_ids.append(torch.tensor([segment_id]))\n","  return zip(input_ids, input_masks, segment_ids)"]},{"cell_type":"markdown","source":["#### kobert 실제 점수 평가"],"metadata":{"id":"LqiRRt6uRn8a"}},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":
n","    \n","    # 실제  값\n","    starts_label = []\n","    ends_lable = []\n","    \n","    # inputs\n","    input_ids, input_mask, segment_ids, cls_index, p_mask, start_positions, end_positions = batch\n","\n","    input_ids = input_ids.to(device)\n","    input_mask = input_mask.to(device)\n","    segment_ids = segment_ids.to(device)\n","    start_positions = start_positions.to(device)\n","    end_positions = end_positions.to(device)\n","    \n","    # train model\n","    # out = model(input_ids=input_ids, attention_mask=attention_mask, labels=labels)\n","    inputs = {'input_ids' : input_ids,\n","              'token_type_ids' : segment_ids,\n","              'attention_mask' : input_mask}\n","    \n","    outputs = model(**inputs)\n","\n","    answer_start_index = outputs.start_logits.argmax(dim=-1)\n","    answer_end_index = outputs.end_logits.argmax(dim=-1)\n","\n","    starts_predicted.extend(answer_start_index.cpu().data.numpy())\n","    ends_predicted.extend(answer_end_index.cpu().data.numpy())\n","\n","    starts_label.extend(start_positions.cpu().data.numpy())\n","    ends_lable.extend(end_positions.cpu().data.numpy())\n","\n","    for i in range(len(starts_predicted)):\n","      phrase = tokenizer.decode(inputs['input_ids'][i].cpu().data.numpy())\n","      questions = phrase.split('[SEP]')[0].replace('[CLS]','')\n","      phrase = phrase.split('[SEP]')[1].replace('[CLS]','')\n","      labels = tokenizer.decode(inputs['input_ids'][i, starts_label[i] : ends_lable[i] + 1])\n","      predicted = tokenizer.decode(inputs['input_ids'][i, starts_predicted[i] : ends_predicted[i] + 1])\n","      \n","      print('\\n-------------')\n","      print('Phrase : {}'.format(phrase))\n","      print('Question : {}'.format(questions))\n","      print('Label : {}'.format(labels))\n","      print('Predicted Answer : {}'.format(predicted))\n","      print('')"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000}
 input_masks = []\n","  segment_ids = []\n","  question_len = len(tokenizer.encode(question)) -2 \n","  context_len = len(tokenizer.encode(context)) -2\n","  if context_len >= 509 - question_len:\n","    cont_len = 509 - question_len\n","    num = int(context_len / cont_len)\n","    for i in range(num+1):\n","      cont_split = tokenizer.encode(context)[1:-1]\n","      if i == 0 :\n","         stride = 0\n","      else:\n","         stride = doc_stride\n","      cont_split = cont_split[i*cont_len - stride : (i + 1)*cont_len - stride]\n","      sequence = tokenizer.encode(question) + cont_split + tokenizer.encode(['[SEP]'])[1:2]\n","      input_mask = [1] * len(sequence)\n","      segment_id = [0] * (question_len+2)+ [1] * (len(cont_split)+1)\n","      input_ids.append(torch.tensor([sequence]))\n","      input_masks.append(torch.tensor([input_mask]))\n","      segment_ids.append(torch.tensor([segment_id]))\n","  else:\n","    sequence = question + '[SEP]' + context\n","    sequence = tokenizer.encode(sequence)\n","    input_mask = [1] * len(sequence)\n","    segment_id = [0] * (question_len+2)+ [1] * (len(sequence) - (question_len+2))\n","    input_ids.append(torch.tensor([sequence]))\n","    input_masks.append(torch.tensor([input_mask]))\n","    segment_ids.append(torch.tensor([segment_id]))\n","  return zip(input_ids, input_masks, segment_ids)"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":442,"referenced_widgets":["9d0e52b845a840f484da7bb007fbcea7","4b41194607274f0e973cc9990958bdf3","7b5633e21e7c452c83d70a339d89841f","20ad4f11234a457ea0a5e8190991cc03","7c1ad407a1164671b6a2fb884ae8cbf3","1cc1bdbeb0c84c748ac302da50e680d8","1fb2b940be774c8d8b8667f12ba082f9","1df947b0e5ce4223ae57444d582db715","3d1471da9424435bbc7c75feabf83e2e","7fa9e010f4204f5fa6e6be471572b73d","b098b9f38f2f480286e89765641cbd25","4a5c12765879415eace1f7abb7e1c4b9","d083b6f1e02c4af28cde88c35e88dcbf","a78302c513b44de2a384121b877430d3","8d7